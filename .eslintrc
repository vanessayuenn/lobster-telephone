{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
    "react/prefer-stateless-function": 0,
    "semi": ["error", "never"],
    "no-param-reassign": ["error", { "props": false }],
    "no-console": ["warn", { allow: ["error"] }],
    "react/prop-types": [1]
  }
}
